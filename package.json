{
  "name": "generolio",
  "version": "0.0.1",
  "description": "Generate web-dev quality portfolio with this no-code solution",
  "main": "index.js",
  "scripts": {
    "dev": "snowpack dev",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "check": "svelte-check --diagnostic-sources \"js,svelte,ts\"",
    "check:watch": "svelte-check --watch --diagnostic-sources \"js,svelte,ts\"",
    "build": "pnpm test && pnpm run check &&snowpack build",
    "serve": "sirv build --maxage 36000"
  },
  "author": "Yonah",
  "license": "MIT",
  "devDependencies": {
    "@snowpack/plugin-svelte": "^3.5.2",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/plugin-webpack": "^2.3.0",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.10",
    "@types/jest": "^26.0.20",
    "@types/snowpack-env": "^2.3.3",
    "autoprefixer": "^10.2.3",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "postcss": "^8.2.15",
    "snowpack": "^3.0.11",
    "svelte": "^3.31.2",
    "svelte-check": "^1.1.28",
    "svelte-hmr": "^0.14.5",
    "svelte-jester": "^1.3.0",
    "svelte-preprocess": "^4.6.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "sirv-cli": "^1.0.10",
    "tailwindcss": "^2.0.2"
  }
}